{
    "name": "trueagency/evo",
    "type": "project",
    "license": "MIT",
    "description": "Modern, opinionated, WordPress boilerplate based on Roots.io",
    "homepage": "https://trueagency.com.au",
    "authors": [
        {
            "name": "Jofry S",
            "email": "jofry@trueagency.com.au",
            "homepage": "https://github.com/jofrysutanto"
        }
    ],
    "keywords": [
        "wordpress",
        "stack",
        "composer",
        "vagrant",
        "wp"
    ],
    "config": {
        "secure-http": false
    },
    "repositories": {
        "0": {
            "type": "composer",
            "url": "https://wpackagist.org"
        },
        "1": {
            "type": "vcs",
            "url": "https://bitbucket.org/trueagencyteam/evolve-engine.git"
        }
    },
    "require": {
        "php": ">=5.5",
        "vlucas/phpdotenv": "^2.0.1",
        "illuminate/support": "5.1.*",
        "illuminate/container": "5.1.*",
        "illuminate/config": "5.1.*",
        "illuminate/http": "5.1.*",
        "october/router": "^1.0",
        "symfony/var-dumper": "^3.2",
        "trueagency/evolve-engine": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    }
}
